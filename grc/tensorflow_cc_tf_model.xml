<?xml version="1.0"?>
<block>
  <name>tf_model</name>
  <key>tensorflow_cc_tf_model</key>
  <category>[tensorflow_cc]</category>
  <import>import tensorflow_cc</import>
  <make>tensorflow_cc.tf_model($model_meta_path,
            $layer_in, $itemsize_in, $vlen_in,
            $layer_out, $itemsize_out, $vlen_out,
            $use_gpu)
  </make>
  <param>
    <name>Model .meta path</name>
    <key>model_meta_path</key>
    <type>string</type>
  </param>
  <param>
    <name>Input layer</name>
    <key>layer_in</key>
    <type>string</type>
  </param>
  <param>
    <name>Input itemsize</name>
    <key>itemsize_in</key>
    <type>int</type>
  </param>
  <param>
    <name>Input vlen</name>
    <key>vlen_in</key>
    <type>int</type>
  </param>
  <param>
    <name>Output layer</name>
    <key>layer_out</key>
    <type>string</type>
  </param>
  <param>
    <name>Output itemsize</name>
    <key>itemsize_out</key>
    <type>int</type>
  </param>
  <param>
    <name>Output vlen</name>
    <key>vlen_out</key>
    <type>int</type>
  </param>
  <param>
    <name>Use GPU</name>
    <key>use_gpu</key>
    <value>True</value>
    <type>enum</type>
    <hide>part</hide>
    <option>
      <name>Yes</name>
      <key>True</key>
    </option>
    <option>
      <name>No</name>
      <key>False</key>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>float</type>
    <vlen>$vlen_in</vlen>
  </sink>
  <source>
    <name>out</name>
    <type>float</type>
    <vlen>$vlen_out</vlen>
  </source>
</block>
